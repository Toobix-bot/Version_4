#############################################
# Environment Example for Evolution Sandbox #
#############################################

# (1) Groq / Model Provider
# Trage hier deinen Groq API Key ein und benenne die Datei in .env um
API_KEY=__DEIN_GROQ_KEY_HIER__
GROQ_MODEL=gemma2-9b-it

# (2) Role Mapping (Alternative zu Single-Key)
# Format token:rolle kommasepariert (rollen: read,write,admin)
# Beispiel: API_TOKENS=reader123:read,writer456:write,root789:admin
API_TOKENS=

# (3) Rate Limit Konfiguration
RATE_LIMIT_PER_MIN=180
DISABLE_RATE_LIMIT=0

# (4) Weitere zuk√ºnftige Provider / Flags
# GROQ_API_KEY kann alternativ zu API_KEY gesetzt werden falls Code angepasst
# TZ / LOG_LEVEL etc.
LOG_LEVEL=info
